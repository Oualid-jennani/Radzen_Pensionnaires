{
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "closeLabel",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Item no longer available.",
                  "type": "label",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "type": "back"
                      }
                    ]
                  },
                  "icon": "",
                  "name": "closeButton",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Close",
                  "type": "button",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "label0",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Another user has made conflicting changes to one or more of the fields you have modified. Please reload.",
                  "type": "label",
                  "visible": "${hasChanges}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "code": "this.load()",
                        "type": "execute"
                      }
                    ]
                  },
                  "icon": "autorenew",
                  "name": "button0",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Reload",
                  "type": "button",
                  "visible": "${hasChanges}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "data": "${questionnaire}",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result.status == 412"
                          },
                          {
                            "name": "canEdit",
                            "type": "set",
                            "value": "!(result.status == 400)"
                          },
                          {
                            "detail": "Unable to update Questionnaire",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "PensionnairesDB.updateQuestionnaire",
                        "parameters": [
                          {
                            "name": "id",
                            "value": "${parameters.id}"
                          },
                          {
                            "name": "Questionnaire",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [
                    {
                      "maxLength": 30,
                      "property": "code",
                      "required": false,
                      "requiredText": "is required",
                      "title": "Code",
                      "type": "string"
                    },
                    {
                      "maxLength": 50,
                      "property": "libelle",
                      "required": false,
                      "requiredText": "is required",
                      "title": "Libelle",
                      "type": "string"
                    },
                    {
                      "maxLength": 200,
                      "property": "description",
                      "required": false,
                      "requiredText": "is required",
                      "title": "Description",
                      "type": "string"
                    },
                    {
                      "property": "active",
                      "required": false,
                      "requiredText": "is required",
                      "title": "Active",
                      "type": "boolean"
                    },
                    {
                      "property": "date_creation",
                      "required": false,
                      "requiredText": "is required",
                      "title": "Date Creation",
                      "type": "date"
                    },
                    {
                      "property": "json",
                      "required": false,
                      "requiredText": "is required",
                      "title": "Json",
                      "type": "text"
                    }
                  ],
                  "name": "form0",
                  "type": "form",
                  "visible": "${canEdit}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true"
      },
      {
        "error": [
          {
            "name": "canEdit",
            "type": "set",
            "value": "!(result.status == 400)"
          }
        ],
        "name": "PensionnairesDB.getQuestionnaireByid",
        "parameters": [
          {
            "name": "id",
            "value": "${parameters.id}"
          }
        ],
        "then": [
          {
            "name": "questionnaire",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Edit Questionnaire"
}